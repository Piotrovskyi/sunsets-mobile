{
  "expo": {
    "name": "sunset",
    "slug": "sunset",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.yojji.sunsets",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "io.yojji.sunsets",
      "versionCode": 1,
      "config" : {
        "googleMaps": {
          "apiKey": ""
        }
      }
    }
  }
}
